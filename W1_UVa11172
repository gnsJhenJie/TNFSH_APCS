#include <iostream>

using namespace std;

int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    int n,a,b;
    cin >> n;
    for (int i=0;i<n;i++){
        cin >> a >> b;
        if (a>b){
            cout << ">\n";
        }else if (b>a){
            cout << "<\n";
        }else{
            cout << "=\n";
        }
    }
    return 0;
}
